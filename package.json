{
    "name": "AnandaMargaPracharakSangha",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
        "rebuild-win": "cd android && gradlew clean && cd.. && yarn bundle && react-native run-android",
        "rebuild-mac": "cd android && ./gradlew clean && cd .. && yarn bundle",
        "and": "react-native run-android",
        "test": "jest",
        "lint": "eslint ."
    },
    "dependencies": {
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/picker": "^1.6.6",
        "@react-native-community/toolbar-android": "^0.1.0-rc.2",
        "@react-native-firebase/app": "^6.7.1",
        "@react-native-firebase/auth": "^6.2.0",
        "@react-native-firebase/database": "^6.2.0",
        "@react-native-firebase/firestore": "^6.7.1",
        "@react-native-firebase/functions": "^6.2.0",
        "@react-native-firebase/in-app-messaging": "^6.2.0",
        "@react-native-firebase/messaging": "^6.2.0",
        "@react-native-firebase/storage": "^6.2.0",
        "@react-navigation/bottom-tabs": "^5.7.3",
        "@react-navigation/drawer": "^5.9.0",
        "@react-navigation/material-bottom-tabs": "^5.2.16",
        "@react-navigation/native": "^5.7.3",
        "@react-navigation/stack": "^5.9.0",
        "firebase": "^7.17.1",
        "install": "^0.13.0",
        "npm": "^6.14.7",
        "pkill": "^2.0.0",
        "react": "16.13.1",
        "react-native": "0.63.2",
        "react-native-action-sheet": "^2.2.0",
        "react-native-elements": "^1.2.7",
        "react-native-fast-image": "^7.0.2",
        "react-native-gesture-handler": "^1.7.0",
        "react-native-google-signin": "^2.1.1",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-media-controls": "^2.0.3",
        "react-native-paper": "^4.0.1",
        "react-native-reanimated": "^1.10.2",
        "react-native-responsive-screen": "^1.4.1",
        "react-native-safe-area-context": "^3.1.4",
        "react-native-screens": "^2.10.1",
        "react-native-slider": "^0.11.0",
        "react-native-svg": "^11.0.1",
        "react-native-tab-view": "^2.12.0",
        "react-native-typography": "^1.4.1",
        "react-native-vector-icons": "^6.6.0",
        "react-native-video": "^5.1.0-alpha8",
        "react-native-webview": "8.1.1",
        "react-navigation": "^4.3.7",
        "react-navigation-stack": "^1.10.3"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "@react-native-community/eslint-config": "^1.0.0",
        "@types/jest": "^25.1.2",
        "@types/react": "^16.9.19",
        "@types/react-native": "^0.61.12",
        "@types/react-native-vector-icons": "^6.4.5",
        "@types/react-test-renderer": "^16.9.2",
        "babel-jest": "^25.1.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "metro-react-native-babel-preset": "^0.58.0",
        "react-native-typescript-transformer": "^1.2.13",
        "react-test-renderer": "16.9.0",
        "ts-jest": "^25.2.0",
        "typescript": "^3.7.5"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
            "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "testPathIgnorePatterns": [
            "\\.snap$",
            "<rootDir>/node_modules/"
        ],
        "cacheDirectory": ".jest/cache"
    }
}
